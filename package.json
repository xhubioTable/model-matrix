{
  "name": "@xhubiotable/model-matrix",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "decision-table",
    "equivalence-class-table",
    "data generator",
    "test data",
    "test"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/xhubioTable/model-matrix.git"
  },
  "bugs": {
    "url": "https://github.com/xhubioTable/model-matrix/issues"
  },
  "homepage": "https://github.com/xhubioTable/model-matrix#readme",
  "scripts": {
    "build": "yarn src_format && yarn src_lint && babel src -d lib",
    "src_lint": "eslint src tests",
    "src_format": "prettier --no-semi --single-quote --trailing-comma es5 --write \"{src,tests}/**/*.js\"",
    "test_only": "jest",
    "test": "yarn build && jest -c jest.config.json",
    "debug": "yarn build && node --inspect-brk ./node_modules/.bin/jest -i --config=./jest.config.json",
    "doc_html": "npx jsdoc -r -c jsdoc.config.json src",
    "doc_md": "npx jsdoc2md -c jsdoc.config.json src/*.js > doc/api.md ",
    "doc": "yarn doc_html && yarn doc_md",
    "prepare": "yarn build"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.1",
    "@babel/eslint-parser": "^7.13.4",
    "@babel/eslint-plugin": "^7.13.0",
    "@babel/node": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.9",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.13.5",
    "@babel/register": "7.13.0",
    "babel-jest": "26.6.3",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "jest-environment-node-debug": "2.0.0",
    "jsdoc": "3.6.6",
    "jsdoc-export-default-interop": "0.3.1",
    "jsdoc-to-markdown": "6.0.1",
    "prettier": "2.2.1",
    "semantic-release": "17.3.9"
  },
  "dependencies": {
    "@babel/runtime": "7.13.7",
    "@xhubiotable/model": "2.4.5"
  }
}
